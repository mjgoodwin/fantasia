%h1.text-center Come in!

= simple_form_for(@form, url: sessions_sign_in_path, as: :session) do |f|
  .row.medium-4
    = f.input :email, placeholder: "Email", label: false
  .row.medium-4
    = f.input :password, placeholder: "Password", label: false
  .row.medium-4
    = f.button :submit, "Enter", class: :expand
